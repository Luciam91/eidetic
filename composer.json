{
  "name": "rawkode/eidetic",
  "description": "EventSourcing library for php",
  "license": "MIT",
  "keywords": [
    "CQRS",
    "EventSourcing"
  ],
  "autoload": {
    "psr-4": {
      "Rawkode\\Eidetic\\": "src//"
    }
  },
  "autoload-dev": {
     "psr-4": {
         "Rawkode\\Eidetic\\Stub\\": "test//stub//"
     }
  },
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "var/vendor"
  },
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~4.3",
    "squizlabs/php_codesniffer": "~2.3"
  }
}
